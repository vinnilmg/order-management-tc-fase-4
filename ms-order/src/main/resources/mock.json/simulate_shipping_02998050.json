{
  "request": {
    "method": "POST",
    "url": "/api/shippings/simulate",
    "headers": {
      "Content-Type": {
        "contains": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "equalToJson": "{\n  \"postalCode\": \"02998050\" \n}",
        "ignoreArrayOrder": true,
        "ignoreExtraElements": true
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "postalCode": "02998050",
      "price": 25.0,
      "daysForDelivery": 3
    }
  }
}
