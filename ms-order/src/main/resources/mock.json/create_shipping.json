{
  "request": {
    "method": "POST",
    "url": "/api/shippings",
    "headers": {
      "Content-Type": {
        "contains": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "equalToJson": "{\n  \"orderId\": 2, \"postalCode\": \"02998050\" \n}",
        "ignoreArrayOrder": true,
        "ignoreExtraElements": true
      }
    ]
  },
  "response": {
    "status": 204,
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
